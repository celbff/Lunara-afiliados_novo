{
  "name": "lunara-afiliados",
  "version": "1.0.0",
  "description": "Sistema integrado Lunara Afiliados + Agenda 2.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "node scripts/setup.js",
    "seed": "node scripts/seed.js",
    "reset": "node scripts/reset.js",
    "backup": "node scripts/backup.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend-lunara && npm run build",
    "install:all": "npm install && cd frontend-lunara && npm install",
    "dev:frontend": "cd frontend-lunara && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:frontend\"",
    "db:reset": "npm run reset && npm run setup && npm run seed",
    "logs": "tail -f logs/app.log",
    "health": "curl http://localhost:5000/health"
  },
  "keywords": [
    "nodejs",
    "express",
    "postgresql",
    "react",
    "affiliates",
    "booking",
    "agenda"
  ],
  "author": "Seu Nome",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "pg": "^8.11.1",
    "uuid": "^9.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "eslint": "^8.44.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/celbff/lunara-afiliados_novo.git"
  },
  "homepage": "https://github.com/celbff/lunara-afiliados_novo#readme"
}
